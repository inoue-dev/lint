{
    "extends": [
        "standard",
        "plugin:prettier/recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    },
    "rules": {
        "prettier/prettier": [
        "error",
            {
                "semi": false,
                "allowEmptyReject": true,
                "prefer-promise-reject-errors": ["error", {"allowEmptyReject": true}]
            }
        ]
    },
    "globals": {

    }
}